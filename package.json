{
    "name": "expressmvc",
    "version": "1.0.0",
    "description": "Simple ES6 Boilderplate for Express.js",
    "main": "app.babel.js",
    "dependencies": {
        "babel-runtime": "^6.23.0",
        "body-parser": "~1.16.0",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.0",
        "express": "~4.14.1",
        "mongoose": "^4.9.2",
        "morgan": "~1.7.0",
        "nodemon": "^1.11.0",
        "pug": "~2.0.0-beta10",
        "serve-favicon": "~2.3.2"
    },
    "devDependencies": {
        "babel-cli": "^6.24.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-es2016": "^6.22.0",
        "babel-register": "^6.26.0",
        "mocha": "^3.5.0",
        "webpack": "^3.5.5"
    },
    "scripts": {
        "test": "mocha 'app/**/*.test.js' --compilers js:babel-core/register --recursive",
        "start": "NODE_ENV=production node ./build/backend.js",
        "run-dev": "DEBUG=express:* nodemon  ./app.js --exec babel-node",
        "build": "rm -r build && NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PedramMarandi/express-js-boilerplate.git"
    },
    "keywords": [
        "expressjs",
        "boilerplate",
        "es6",
        "javascript",
        "MVC"
    ],
    "author": "Pedram Marandi",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/PedramMarandi/express-js-boilerplate/issues"
    },
    "homepage": "https://github.com/PedramMarandi/express-js-boilerplate#readme"
}
